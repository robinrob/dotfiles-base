local +r Alias=$1

local +r ResolvedAlias="$(resolve_alias $Alias)"

local +r Which="$(which $ResolvedAlias)"

local +r BadResult=$(print $Which | ggrep "$ResolvedAlias not found")

local +r ErrorMsg="$(yellow $Alias) $(red 'resolves to an unknown command!')"


# alias resolved to nothing
if is_empty "$Which"
then
  print $ErrorMsg

# `which` resolves to no command
elif is_not_empty $BadResult
then
  print $ErrorMsg

# `which` resolves to a command
else
  print $Which
fi
