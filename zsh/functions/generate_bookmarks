local -r bookmarks_path=$1
local -r bookmarks_alias_path=$2

rm -f $bookmarks_alias_path && touch $bookmarks_alias_path

cat $bookmarks_path | while read line ; do

  local -r PARTS=("${(s/::/)line}")

  local -r NAME=$PARTS[1]
  local -r BROWSER=$PARTS[2]
  local -r URL=$PARTS[3]

  local -r  ALIAS="browse_copy $BROWSER '$URL'"

  create_alias $NAME $ALIAS $bookmarks_alias_path $SUCCESS_MSG

  source $bookmarks_alias_path

done
