rm -f $BOOKMARKS_ALIAS_PATH.old
mv $BOOKMARKS_ALIAS_PATH $BOOKMARKS_ALIAS_PATH.old
touch $BOOKMARKS_ALIAS_PATH

cat $BOOKMARKS_PATH | while read line ; do

  PARTS=("${(s/::/)line}")

  NAME=$PARTS[1]
  BROWSER=$PARTS[2]
  URL=$PARTS[3]

  ALIAS="browse_print $BROWSER '$URL'"

  create_alias $NAME $ALIAS $BOOKMARKS_ALIAS_PATH $SUCCESS_MSG

  source $BOOKMARKS_ALIAS_PATH

done
